<?xml version="1.0" encoding="UTF-8" ?>

<Shell
    x:Class="Something.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Something"
    Shell.FlyoutBehavior="Disabled"
>
    
    <TabBar >
        <Tab Icon="homeicon.svg">
            
            <ShellContent ContentTemplate="{DataTemplate local:EnterPage}"/>
        </Tab>
        <Tab  Icon="listicon.svg">
            <ShellContent ContentTemplate="{DataTemplate local:List}"/>
        </Tab>
   
        
    </TabBar>
    

</Shell>
